		>;
	};

	mcu_lvds_power_pins_default: mcu-lvds-power-pins-default {
		pinctrl-single,pins = <
			AM62X_MCU_IOPAD(0x0010, PIN_OUTPUT_PULLUP, 7) /* (C9) MCU_SPI0_D1.MCU_GPIO0_4 */
